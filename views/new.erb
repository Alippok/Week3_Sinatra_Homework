<form action="/bookmarker" method="post">
  <legend>Create a bookmark</legend>
    <label>URL:</label>
    <input type="text" name="url">
    <label>Title:</label>
    <input type="text" name="title">
    <label>Genre:</label>
    <input type="text" name="genre">
    <label>Details:</label>
    <input type="text" name="details">

    <button type="submit">Place my bookmark</button>

</form>